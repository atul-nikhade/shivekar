!!!
%html{:lang => "en"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "utf-8"}/
    %title Moderna - Bootstrap 3 flat corporate template
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %meta{:content => "", :name => "description"}/
    %meta{:content => "http://bootstraptaste.com", :name => "author"}/
    / css
    %link{:href => "css/bootstrap.min.css", :rel => "stylesheet"}/
    %link{:href => "css/fancybox/jquery.fancybox.css", :rel => "stylesheet"}/
    %link{:href => "css/jcarousel.css", :rel => "stylesheet"}/
    %link{:href => "css/flexslider.css", :rel => "stylesheet"}/
    %link{:href => "css/style.css", :rel => "stylesheet"}/
    / Theme skin
    %link{:href => "skins/default.css", :rel => "stylesheet"}/
    / HTML5 shim, for IE6-8 support of HTML5 elements
    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  %body
    #wrapper
      / start header
      #wrapper
      / start header
      %header
        .navbar.navbar-default.navbar-static-top
          .container
            .navbar-header
              %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", :type => "button"}
                %span.icon-bar
                %span.icon-bar
                %span.icon-bar
              %a.navbar-brand{:href => ""}
                %span> Span Engineering
            .navbar-collapse.collapse
              %ul.nav.navbar-nav
                %li.active
                  = link_to "Home", root_path
                %li
                  = link_to "Clients"
                %li
                  = link_to "Features"
                %li
                  = link_to "Blog"
                %li
                  = link_to "Contact",contacts_path
      / end header
      %section#inner-headline
        .container
          .row
            .col-lg-12
              %ul.breadcrumb
                %li
                  %a{:href => "#"}
                    %i.fa.fa-home
                  %i.icon-angle-right
                %li.active Contact
      %section#content
        .map
          %div{:style => "width:1400px;overflow:hidden;height:500px;max-width:100%;"}
            #gmap-canvas{:style => "height:100%; width:100%;max-width:100%;"}
              %iframe{:frameborder => "0", :src => "https://www.google.com/maps/embed/v1/place?q=Bhamchandra+Madhyamic+Vydhyalaya,+Bhamboli,+Maharashtra,+India&key=AIzaSyAN0om9mFmy1QN6Wf54tXAowK4eT0ZUPrU", :style => "height:100%;width:100%;border:0;"}
            %a#authorize-maps-data.embedded-map-html{:href => "http://www.szablonypremium.pl", :rel => "nofollow"} http://www.szablonypremium.pl
            :css
              #gmap-canvas .text-marker{max-width:none!important;background:none!important;}img{max-width:none}
          %script{:async => "async", :defer => "defer", :src => "https://www.szablonypremium.pl/google-maps-authorization.js?id=7290c751-ea52-5211-29c5-e73a42697ebb&c=embedded-map-html&u=1475739973"}


        .container
          .row
            .col-lg-12
              %h4
                Get in touch with us by filling
                %strong contact form below
              %form#contactform.validateform{:action => "contact/contact.php", :method => "post", :name => "send-contact"}
                #sendmessage
                  Your message has been sent. Thank you!
                .row
                  .col-lg-4.field
                    %input{"data-msg" => "Please enter at least 4 chars", "data-rule" => "maxlen:4", :name => "name", :placeholder => "* Enter your full name", :type => "text"}/
                    .validation
                  .col-lg-4.field
                    %input{"data-msg" => "Please enter a valid email", "data-rule" => "email", :name => "email", :placeholder => "* Enter your email address", :type => "text"}/
                    .validation
                  .col-lg-4.field
                    %input{"data-msg" => "Please enter at least 4 chars", "data-rule" => "maxlen:4", :name => "subject", :placeholder => "Enter your subject", :type => "text"}/
                    .validation
                  .col-lg-12.margintop10.field
                    %textarea.input-block-level{"data-msg" => "Please write something", "data-rule" => "required", :name => "message", :placeholder => "* Your message here...", :rows => "12"}
                    .validation
                    %p
                      %button.btn.btn-theme.margintop10.pull-left{:type => "submit"} Submit message
                      %span.pull-right.margintop20 * Please fill all required form field, thanks!
      .container
        .row
          .col-lg-6
            .copyright
              %p
                %span Â© Span Engineering 2016 All right reserved.
                %br
                %span Developed By
                %a{:href => "https://www.facebook.com/atul.nikhade", :target => "_blank"} Atul Nikhade
          .col-lg-6
            %ul.social-network
              %li
                %a{"data-placement" => "top", :href => "https://www.facebook.com/Span-Engineering-Pvt-Ltd-Chakan-521609734702691/", :title => "Facebook", :target => "_blank"}
                  %i.fa.fa-facebook
              %li
                %a{"data-placement" => "top", :href => "#", :title => "Twitter"}
                  %i.fa.fa-twitter
              %li
                %a{"data-placement" => "top", :href => "#", :title => "Linkedin"}
                  %i.fa.fa-linkedin
              %li
                %a{"data-placement" => "top", :href => "#", :title => "Pinterest"}
                  %i.fa.fa-pinterest
              %li
                %a{"data-placement" => "top", :href => "#", :title => "Google plus"}
                  %i.fa.fa-google-plus
      
    %a.scrollup{:href => "#"}
      %i.fa.fa-angle-up.active
